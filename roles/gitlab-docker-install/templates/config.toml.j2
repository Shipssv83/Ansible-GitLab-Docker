[[runners]]
  name = "docker-runner"
  url = "http://gitlab:80/"
  token = "{{ registrattion_token }}"
  executor = "docker"
  tls_skip_verify = true
  [runners.custom_build_dir]
  [runners.custom_cache_dir]
  [runners.docker]
    tls_verify = false
    image = "docker:stable"
    privileged = true
    disable_entrypoint_overwrite = false
    oom_kill_disable = false
    disable_cache = false
    volumes = ["/var/run/docker.sock:/var/run/docker.sock", "/cache"]
    shm_size = 0
  [runners.cache]
